<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="redisConfig" class="com.milesbone.cache.redis.config.DefaultRedisConfig"/>
	
	<bean id="redisClusterImpl" class="com.milesbone.cache.redis.service.impl.RedisClusterImpl">
		<constructor-arg name="redisConfig" ref="redisConfig"></constructor-arg>
	</bean>
	
	
</beans>
